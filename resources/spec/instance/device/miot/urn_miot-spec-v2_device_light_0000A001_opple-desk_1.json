{
  "type": "urn:miot-spec-v2:device:light:0000A001:opple-desk:1",
  "description": "Light",
  "services": [
    {
      "iid": 1,
      "type": "urn:miot-spec-v2:service:device-information:00007801:opple-desk:1",
      "description": "Device Information",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:manufacturer:00000001:opple-desk:1",
          "description": "Device Manufacturer",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 2,
          "type": "urn:miot-spec-v2:property:model:00000002:opple-desk:1",
          "description": "Device Model",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 3,
          "type": "urn:miot-spec-v2:property:serial-number:00000003:opple-desk:1",
          "description": "Device Serial Number",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 4,
          "type": "urn:miot-spec-v2:property:name:00000004:opple-desk:1",
          "description": "Device Name",
          "format": "string",
          "access": [
            "read"
          ]
        },
        {
          "iid": 5,
          "type": "urn:miot-spec-v2:property:firmware-revision:00000005:opple-desk:1",
          "description": "Current Firmware Version",
          "format": "string",
          "access": [
            "read"
          ]
        }
      ],
      "actions": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:action:identify:00002801:opple-desk:1",
          "description": "Identify"
        }
      ]
    },
    {
      "iid": 2,
      "type": "urn:miot-spec-v2:service:light:00007802:opple-desk:1",
      "description": "Light",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:on:00000006:opple-desk:1",
          "description": "Switch Status",
          "format": "bool",
          "access": [
            "read",
            "write",
            "notify"
          ]
        },
        {
          "iid": 2,
          "type": "urn:miot-spec-v2:property:mode:00000008:opple-desk:1",
          "description": "Mode",
          "format": "uint8",
          "access": [
            "read",
            "write",
            "notify"
          ],
          "value-list": [
            {
              "value": 0,
              "description": "Auto"
            }
          ]
        },
        {
          "iid": 3,
          "type": "urn:miot-spec-v2:property:brightness:0000000D:opple-desk:1",
          "description": "Brightness",
          "format": "uint8",
          "access": [
            "read",
            "write",
            "notify"
          ],
          "value-range": [
            0,
            100,
            1
          ],
          "unit": "percentage"
        },
        {
          "iid": 4,
          "type": "urn:miot-spec-v2:property:color:0000000E:opple-desk:1",
          "description": "Color",
          "format": "uint32",
          "access": [
            "read",
            "write",
            "notify"
          ],
          "value-range": [
            0,
            16777215,
            1
          ],
          "unit": "rgb"
        },
        {
          "iid": 5,
          "type": "urn:miot-spec-v2:property:color-temperature:0000000F:opple-desk:1",
          "description": "Color Temperature",
          "format": "uint32",
          "access": [
            "read",
            "write",
            "notify"
          ],
          "value-range": [
            1000,
            10000,
            1
          ],
          "unit": "kelvin"
        },
        {
          "iid": 6,
          "type": "urn:miot-spec-v2:property:flow:00000010:opple-desk:1",
          "description": "Flow",
          "format": "bool",
          "access": [
            "read",
            "write",
            "notify"
          ]
        },
        {
          "iid": 7,
          "type": "urn:miot-spec-v2:property:saturability:00000011:opple-desk:1",
          "description": "Saturability",
          "format": "uint8",
          "access": [
            "read",
            "write",
            "notify"
          ],
          "value-range": [
            0,
            100,
            1
          ],
          "unit": "percentage"
        }
      ]
    },
    {
      "iid": 3,
      "type": "urn:miot-spec-v2:service:battery:00007805:opple-desk:1",
      "description": "Battery",
      "properties": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:property:battery-level:00000014:opple-desk:1",
          "description": "Battery Level",
          "format": "uint8",
          "access": [
            "read",
            "notify"
          ],
          "value-range": [
            0,
            100,
            1
          ],
          "unit": "percentage"
        },
        {
          "iid": 2,
          "type": "urn:miot-spec-v2:property:charging-state:00000015:opple-desk:1",
          "description": "Charging State",
          "format": "uint8",
          "access": [
            "read",
            "notify"
          ],
          "value-list": [
            {
              "value": 1,
              "description": "Charging"
            },
            {
              "value": 2,
              "description": "Not charging"
            },
            {
              "value": 3,
              "description": "Not chargeable"
            }
          ]
        }
      ],
      "actions": [
        {
          "iid": 1,
          "type": "urn:miot-spec-v2:action:start-charge:00002802:opple-desk:1",
          "description": "Start Charge"
        }
      ]
    }
  ]
}